{% extends 'User/User.html.twig' %}
{% block bodyakhel %}
    <div class="tg-banner tg-haslayout">
        <div class="tg-imglayer">
            <img src="{{asset('images/bg-pattran.png')}}" alt="image desctription">
        </div>
        <div class="container">
            <div class="row">
                <div class="tg-banner-content tg-haslayout">
                    <div class="tg-pagetitle">
                        <h1>Ajouter un membre</h1>
                    </div>
                    <ol class="tg-breadcrumb">

                        <li class="active">Ajouter un membre</li>
                    </ol>
                </div>
            </div>
        </div>
    </div>
{% endblock %}
{% block body %}

    <main id="tg-main" class="tg-main tg-haslayout">
        <!--************************************
                Top Rated Player Start
        *************************************-->

        <section class="tg-main-section tg-haslayout">
            <div class="container">
                <div class="tg-section-name">
                    <h2>Ajouter un membre</h2>
                </div>
            </div>
            <div class="col-sm-push-4 col-xs-11 pull-right">
                <div class="row">
                    <h6 style="color: firebrick">Email user</h6>
                    <div class="form-group">
                        {{ form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}
                        {{ form_widget(form.Email, { 'attr': {'class': 'single-input','placeholder': ' Email d utilisateur'} }) }}
                        {{ form_widget(form.Valider, { 'attr': {'class': 'tg-btn'} }) }}
                        {{ form_end(form) }}
                        <h5 style="color:red">{{ err }}</h5>
                    </div>
                </div>
            </div>
        </section>
    </main>


{% endblock %}