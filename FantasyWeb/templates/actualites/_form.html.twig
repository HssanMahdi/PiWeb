<div classe="countainer mt-4">

    {{ form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}

    <div class="row">
        <div class="col-md-4">{{ form_row(form.titre) }}</div>
        <div class="col-md-4">{{ form_row(form.description) }}</div>
        <div class="col-md-4">{{ form_row(form.date) }}</div>
        <div class="col-md-3"> {{ form_row(form.imageFile) }}</div>
    </div>

    {{ form_widget(form) }}
    <button class="btn">{{ button_label|default('Save') }}</button>
    {{ form_end(form) }}


</div>
