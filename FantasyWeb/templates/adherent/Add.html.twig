{% block body %}

    <main id="tg-main" class="tg-main tg-haslayout">
        <!--************************************
                Top Rated Player Start
        *************************************-->

        <section class="tg-main-section tg-haslayout">
            <div class="container">
                <div class="tg-section-name">
                    <h2>Inscription</h2>
                </div>
            </div>
            <div class="col-md-8 col-sm-9 col-xs-2 pull-right">
                    <div class="row">
                         {{ form_start(form,{'attr': {'novalidate': 'novalidate'}} ) }}
                         <h6 style="color:#AB0000;">Nom</h6>
                          <div class="form-group">
                           {{ form_errors(form.email) }}
                               {{ form_widget(form.nomUser, { 'attr': {'class': 'form-control','placeholder': 'Entrer Votre Nom'} }) }}
                           </div>
                            <h6 style="color:#ab0000;">Email</h6>
                            <div class="form-group">
                             {{ form_errors(form.email) }}
                                {{ form_widget(form.email, { 'attr': {'class': 'form-control','placeholder': 'Entrer Votre Email'} }) }}
                            </div>
                            <h6 style="color:#ab0000;">Mot de passe</h6>
                            <div class="form-group">
                             {{ form_errors(form.email) }}
                                {{ form_widget(form.password, { 'attr': {'class': 'form-control','placeholder': 'Entrer Votre Mdp'} }) }}
                             </div>
                             <h6 style="color:#ab0000;">Confirmation Mdp</h6>
                             <div class="form-group">
                              {{ form_errors(form.email) }}
                                 {{ form_widget(form.confirm_password, { 'attr': {'class': 'form-control','placeholder': 'Valider Votre Mdp'} }) }}
                             </div>
                             {{ form_widget(form.Valider, { 'attr': {'class': 'btn btn-outline-danger'} }) }}

                    {{ form_end(form) }}
                    </div>
            </div>
        </section>
    </main>


